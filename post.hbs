{{!< default}}
<div id="post-nav"> <!-- post navigation -->
</div> <!-- close post navigation -->
<div class="post"> <!-- container for posts -->
{{#post}}
<h2><a href="{{url}}" title="{{{title}}}">{{{title}}}</a></h2>
<div class="date">{{date published_at format="MMMM DD, YYYY"}}</div>
<div class="entry">{{content}}</div>
<div class="meta">
	<div class="tags">
	{{#tags}}
<li>{{nane}}</li>
{{/tags}}
    </div>
</div> <!-- close meta space -->
{{/post}}
</div> <!-- close post container -->
